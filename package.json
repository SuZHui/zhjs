{
  "name": "zhjs",
  "version": "0.1.10",
  "description": "",
  "type": "module",
  "main": "dist/zhjs.es.js",
  "types": "dist/types/zhjs.d.ts",
  "typings": "dist/types/zhjs.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/zhjs.d.ts",
        "default": "./dist/zhjs.es.js"
      },
      "module": {
        "types": "./dist/types/zhjs.d.ts",
        "default": "./dist/zhjs.es.js"
      },
      "default": {
        "types": "./dist/types/zhjs.d.ts",
        "default": "./dist/zhjs.umd.js"
      }
    },
    "./array": {
      "import": {
        "types": "./dist/types/array/index.d.ts",
        "default": "./dist/dist/zhjs_array.es.js"
      },
      "module": {
        "types": "./dist/types/array/index.d.ts",
        "default": "./dist/dist/zhjs_array.es.js"
      },
      "default": {
        "types": "./dist/types/array/index.d.ts",
        "default": "./dist/zhjs_array.umd.js"
      }
    }
  },
  "files": [
    "dist/*",
    "package.json"
  ],
  "scripts": {
    "_clean": "rimraf dist",
    "_build:dts": "tsc -p tsconfig.dts.json && rollup -c rollup.config.dts.mjs && rimraf _temp",
    "build": "rollup -c",
    "dev": "rollup -w -c",
    "test": "jest",
    "prebuild": "npm run _clean",
    "postbuild": "npm run _build:dts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SuZHui/zhjs.git"
  },
  "keywords": [],
  "author": "su zhen hui<szh362680581@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SuZHui/zhjs/issues"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "homepage": "https://github.com/SuZHui/zhjs#readme",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/jest": "^29.5.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "rollup": "^4.9.1",
    "rollup-plugin-dts": "^6.1.0",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "rxjs": "^7.8.1",
    "umi-request": "^1.4.0"
  }
}
